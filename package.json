{
  "name": "itools-sms-viewer",
  "version": "3.0.0",
  "author": "Val Saven <val.saven@gmail.com>",
  "description": "SMS viewer for iTools Super Backup",
  "private": true,
  "license": "GPL-3.0",
  "main": "dist/main/index.cjs",
  "scripts": {
    "dev": "node scripts/watch.mjs",
    "prebuild": "vue-tsc --noEmit --p packages/renderer/tsconfig.json && node scripts/build.mjs",
    "build": "electron-builder"
  },
  "engines": {
    "npm": ">=9.0.0",
    "node": ">=20.0.0"
  },
  "dependencies": {
    "sqlite3": "5.1.6"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "4.2.3",
    "electron": "24.3.0",
    "electron-builder": "24.4.0",
    "lib-esm": "0.4.0",
    "typescript": "5.0.4",
    "vite": "4.3.9",
    "vite-plugin-electron": "0.11.2",
    "vite-plugin-electron-renderer": "0.14.2",
    "vite-plugin-resolve": "2.3.0",
    "vue": "3.3.2",
    "vue-tsc": "1.6.5"
  },
  "env": {
    "VITE_DEV_SERVER_HOST": "127.0.0.1",
    "VITE_DEV_SERVER_PORT": 3344
  },
  "keywords": [
    "backup",
    "electron",
    "iphone",
    "itools",
    "rollup",
    "sms",
    "viewer",
    "vite",
    "vue",
    "vue3"
  ]
}
